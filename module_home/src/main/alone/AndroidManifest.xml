<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.lcworld.module_home">

    <application
        android:name="com.lcworld.library_base.debug.DebugApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/home_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppMainTheme"
        tools:replace="label">
        <activity
            android:name="com.lcworld.module_home.debug.DebugActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.lcworld.module_home.activity.SearchGoodsAct" />
        <activity android:name="com.lcworld.module_home.activity.SearchGoodsResultAct" />
        <activity android:name="com.lcworld.module_home.activity.GoodsHotSaleListAct" />
        <activity android:name="com.lcworld.module_home.activity.MessageListAct" />
        <activity android:name="com.lcworld.module_home.activity.MessageDetailAct"/>
        <activity android:name="com.lcworld.module_home.activity.OperatorApplyAct" />
        <activity android:name="com.lcworld.module_home.activity.OperatorAreaLocationAct"/>
        <activity android:name="com.lcworld.module_home.activity.OperatorAuditResultAct"/>

    </application>
</manifest>